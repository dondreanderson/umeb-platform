{
    "buildCommand": "cd frontend && npm install && npm run build",
    "installCommand": "npm install",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        },
        {
            "source": "/docs",
            "destination": "/api/index.py"
        },
        {
            "source": "/openapi.json",
            "destination": "/api/index.py"
        }
    ]
}